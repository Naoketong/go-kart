}


.add-personnel-item{
	display: flex;
	align-items:center;s
	width: 100%;
	height: 88rpx;
	background-color: #fff;
	padding:0 40rpx;
	border:1px solid rgba(131,129,126,0.2);
}
.add-personnel-img{
	display: inline-block;
