page{
	background:rgba(244,245,246,1);
	width: 100%;
	height: 100%;
}
.page-container{
	width: 100%;
	height: 100%;
}

.head-inputbox{
	position: relative;
	display: flex;
	justify-content:center;
	align-items: center;
	width: 100%;
	height:116rpx;
	background-color: #fff;
	border-bottom: 1px solid rgba(238,238,238,1);
}
.head-inputbox.active{
	justify-content: flex-start;
}
.head-inputbox.active .head-input{
	width: 590rpx;
	margin-left: 32rpx;
} 
.head-input{
	width:702rpx;
	height:80rpx;
	background:rgba(244,245,246,1);
	border-radius:8rpx;
	font-size:28rpx;
	font-family:PingFangSC-Regular;
	font-weight:400;
	color:rgba(166,169,171,1);
	line-height:40rpx;
	padding-left: 60rpx;
}
.head-input-icon{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	left: 52rpx;
	width: 28rpx;
	height: 28rpx;
	z-index: 2;
}
.head-input-icondel{
	display: none;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 156rpx;
	width: 28rpx;
	height: 28rpx;
	z-index: 2;
	cursor: pointer;
}
.head-inputbox.active .head-input-icondel{
	display: inline-block;
}
.headinput-cancel{
	display: none;
	font-size:32rpx;
	font-family:PingFangSC-Regular;
	font-weight:500;
	color:rgba(255,111,32,1);
	line-height:45rpx;
	margin-left: 32rpx;
	cursor: pointer;
}
.head-inputbox.active .headinput-cancel{
	display: inline-block;
}

.personnel-list{/*阴影增加位置*/
	position: relative;
	font-size: 0;
}
.personnel-item{
	display: inline-block;
	position: relative;
	height: 159rpx;
	/*right:160rpx;*/
	/*删除向左滑动160rpx*/
}
.personnel-item-left{
	display: inline-block;
	width: 750rpx;
	height: 100%;
	background-color: #fff;
	padding:40rpx 50rpx;
	overflow: hidden;
}
.personnel-item-left:before{
	position: absolute;
	bottom: 1rpx;
	right: 30rpx;
	content: '';
	display: inline-block;
	width:572rpx;
	height:1rpx;
	background:rgba(238,238,238,1);
}
.personnel-img{
	display: inline-block;
	width: 80rpx;
	height: 80rpx;
}
.personnel-item-text{
	display: inline-block;
	width: ;
	margin-left: 16px;
	vertical-align: top;
}
.personnel-text-english{
