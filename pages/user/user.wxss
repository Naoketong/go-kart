	height: 32rpx;
}






/*遮罩层*/
.page-shade{
	display: none;
}
.page-container.active .page-shade{
	display: inline-block;
	width: 100%;
	height: 100%;

	background-color: rgba(0,0,0,0.6); 
	position: fixed;
	top: 0rpx;
	left: 0rpx;
	z-index: 900;
}
.shade-section{
	width: 560rpx;
	/*height: 265rpx;*/
	background-color: #fff;
	position: absolute;
	left: 50%;
	top:407rpx;
	transform: translateX(-50%);
}
.shade-title{
	width: 100%;
	border-bottom: 1rpx solid #E5E5E5;
	text-align: center;
	padding:50rpx 30rpx 30rpx 44rpx;
	font-size:30rpx;
	font-family:PingFangSC-Regular;
	font-weight:500;
	color:rgba(45,44,44,1);
	line-height:42rpx;
}
.shade-name{
	color:rgba(255,111,32,1);
}
.shade-button .shade-button{
	display: inline-block;
	width: 50%;
	padding:25rpx 0;
	font-size:36rpx;
	font-family:PingFangSC-Regular;
	font-weight:500;
	color:rgba(45,44,44,1);
	line-height:50rpx;
	text-align: center;
}
.shade-button .affirm{
	color:rgba(255,111,32,1); 
	border-left: 1rpx solid #E5E5E5;
}